    "istanbul": "^0.4.4",
    "joi": "6.1.2",
    "make-up": "^9.3.1",
    "mocha": "^3.0.2",
    "sinon": "^1.17.4",
    "sinon-as-promised": "^4.0.2",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "boom": "2.6.1",
