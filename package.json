{
  "name": "plugin-jsonapi",
  "description": "A hapi plugin for a jsonapi style output according to our 'house rules'",
  "version": "2.0.0",
  "homepage": "https://github.com/holidayextras/plugin-jsonapi",
  "author": {
    "name": "Shortbreaks",
    "email": "shortbreaksdev@holidayextras.com"
  },
