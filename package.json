  "main": "index",
  "engines": {
    "node": ">= 4.4.4"
  },
  "scripts": {
    "ci": "node_modules/.bin/istanbul cover _mocha -- -R xunit > $CIRCLE_TEST_REPORTS/unit/results.xml",
